{
    "scripts": {
        "testChrome": "testcafe chrome tests/*",
        "testFirefox": "testcafe firefox tests/*",
        "testSafari": "testcafe safari tests/*",
        "testReport": "testcafe chrome tests/* --reporter html:reports/testingReport.html",
        "testAllBrowsers": "npm run testChrome && npm run testFirefox && npm run testReport",
        "testWithReport": "npm run testReport"
    },
    "dependencies": {
        "testcafe": "^1.9.4",
        "testcafe-reporter-html": "^1.4.6",
        "testcafe-reporter-json": "^2.2.0",
        "testcafe-reporter-list": "^2.1.0",
        "testcafe-reporter-minimal": "^2.1.0",
        "testcafe-reporter-spec": "^2.1.1",
        "testcafe-reporter-teamcity": "^1.0.12",
        "testcafe-reporter-tesults": "^1.1.0",
        "testcafe-reporter-xunit": "^2.1.0"
    }
}
